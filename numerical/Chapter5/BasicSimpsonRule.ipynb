   "source": [
    "import numpy as np\n",
    "\n",
    "def BasicSimsonRule(f,a,b,n):\n",
    "    h = (b-a)/n\n",
    "    x = np.linspace(a,b,n+1)\n",
    "    y = f(x)\n",
    "    I = h/3*(y[0]+y[-1]+4*sum(y[1:-1:2])+2*sum(y[2:-1:2]))\n",
    "    return I\n",
    "\n",
    "def f(x):\n",
    "    return x**2\n",
    "\n",
    "a = 0\n",
    "b = 1\n",
    "n = 100\n",
    "I = BasicSimsonRule(f,a,b,n)\n",
    "print(I)\n"
   ]
  }
