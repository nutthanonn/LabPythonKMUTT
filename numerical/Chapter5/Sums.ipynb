   "source": [
    "import math\n",
    "\n",
    "def Sums(a, b, f, n=10):\n",
    "    h = (b-a)/n\n",
    "    sum = 0\n",
    "    for i in range(n, 0, -1):\n",
    "        x = a+i*h\n",
    "        sum += f(x)\n",
    "    sum_lower = sum*h\n",
    "    sum_upper = sum_lower + h*(f(a) - f(b))\n",
    "\n",
    "    return sum_lower, sum_upper\n",
    "\n",
    "def f(x): return math.exp(-x**2)\n",
    "\n",
    "Sums(0,1,f)"
   ]
  }
